# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  user: User!
  posts(cursor: String): Posts
  post(id: String!): Post!
  comments(post_id: String!, cursor: String): Comments

  wallet: Wallet
  walletEvents(cursor: String): WalletEvents

  depositOrder(id: String!): DepositOrder!
}
