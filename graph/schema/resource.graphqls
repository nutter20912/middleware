type Paginator {
  next_cursor: String
}

type User {
  id: ID!
  name: String!
  email: String!
  created_at: String!
}

type Posts {
  data: [Post]!
  paginator: Paginator
}

type Post {
  id: ID!
  title: String!
  content: String!
  user_id: ID!

  user: User!
}

type Comments {
  data: [Comment]!
  paginator: Paginator
}

type Comment {
  id: ID!
  user_id: ID!
  content: String!

  user: User!
}
